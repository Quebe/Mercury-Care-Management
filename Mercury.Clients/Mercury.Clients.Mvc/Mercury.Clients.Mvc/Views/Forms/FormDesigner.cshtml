<script type="text/javascript">

    $(window).resize(function () {

        var availableHeight = $(window).height();

        availableHeight = availableHeight - $("#ApplicationTitleBar").outerHeight();

        availableHeight = availableHeight - $("#RibbonTabs").outerHeight();

        if (availableHeight < 0) { availableHeight = 1; }

        $("#ApplicationContent").height(availableHeight);

        return;

    });


    var uilayoutControlSettings = {

        name: "DesignerLayout",

        defaults: {

            size: "auto",

            spacing_open: 3,

            togglerClass: "toggler", 

            buttonClass: "button",

            resizeable: false,

            slidable: false


        },

        north: {

            spacing_open: 0,

            togglerLength_open: 0,

            togglerLength_closed: -1,

            resizeable: false,

            slidable: false

        },

        west: {

            size: 160

        },

        east: {

            size: 325

        }


    };



    $(document).ready(function () {
            
        $(window).resize();

        $("#RibbonTabs").tabs();

        $("#RibbonTabs").removeClass("ui-corner-all");

        $("#RibbonTabs ul").removeClass("ui-corner-all");

        $("#RibbonTabs .ui-tabs-panel").removeClass("ui-corner-bottom");


        $("#HomeToolbar").toolbar();

        $("#StyleToolbarControlSelection").toolbar();

        $("#StyleToolbarFont1").toolbar();

        $("#StyleToolbarFont2").toolbar();

        $("#StyleToolbarParagraph1").toolbar();


